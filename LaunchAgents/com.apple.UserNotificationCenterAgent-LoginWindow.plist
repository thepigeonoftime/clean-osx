<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.apple.UserNotificationCenterAgent-LoginWindow</string>
	<key>ProgramArguments</key>
	<array>
		<string>/System/Library/CoreServices/UserNotificationCenter.app/Contents/MacOS/UserNotificationCenter</string>
		<string>-loginwindow</string>
	</array>
	<key>MachServices</key>
	<dict>
		<key>com.apple.UNCUserNotificationAgent</key>
		<true/>
	</dict>
	<key>LimitLoadToSessionType</key>
	<string>LoginWindow</string>
	<key>KeepAlive</key>
	<dict>
		<key>PathState</key>
		<dict>
			<key>/var/run/com.apple.UserNotificationCenterAgent</key>
			<true/>
		</dict>
	</dict>
</dict>
</plist>
